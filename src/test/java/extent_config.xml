<?xml version="1.0" encoding="UTF-8"?>
<extentreports xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="extent_config.xsd">
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Extent Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Automation Test Result</reportName>

		<!-- view report without internet -->
		<offlineMode>false</offlineMode>

		<thumbnailForBase64>true</thumbnailForBase64>
		<testViewChartLocation>bottom</testViewChartLocation>
		<!-- custom javascript -->
		<js></js>
		<scripts>

<![CDATA[
$(document).ready(function() {
});
]]>
		</scripts>
		<!-- custom styles -->
		<styles>
<![CDATA[
]]>
		</styles>

		<!-- custom styles -->
		<css></css>
	</configuration>
</extentreports>